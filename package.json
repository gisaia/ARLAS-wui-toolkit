    "gulp-clean": "^0.4.0",
    "gulp-sourcemaps": "^2.6.0",
    "jasmine-core": "~3.10.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^6.3.4",
    "karma-chrome-launcher": "~3.1.0",
