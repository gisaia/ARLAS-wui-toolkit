    "node": "^16.3.0",
    "patch-package": "^6.4.7",
    "protractor": "~7.0.0",
    "ts-node": "~10.4.0",
    "typescript": "~4.6.0"
  },
  "browser": {
    "jsonpath": false
  },
  "overrides": {
